<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>{% block title %}Demo Bundle{% endblock %}</title>

    <link rel="stylesheet/less" type="text/css" href="{{ asset('bundles/pixelcore/less/style.less') }}">

    <script src="{{ asset('bundles/pixelcore/js/less-1.2.2.min.js') }}" type="text/javascript"></script>

    <link rel="shortcut icon" href="{{ asset('favicon.ico') }}" />
</head>
<body>

    <div class="container">

        <header class="main-header">
            <div class="row">
                <div class="span12">

                    <div class="navbar navbar-fixed-top">
                        <div class="navbar-inner">
                            <div class="container">
                                <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                </a>
                                <a class="brand" href="./index.html">Bootstrap</a>
                                <div class="nav-collapse">
                                    <ul class="nav">
                                        <li class="">
                                            <a href="./index.html">Overview</a>
                                        </li>
                                        <li class="">
                                            <a href="./scaffolding.html">Scaffolding</a>
                                        </li>
                                        <li class="">
                                            <a href="./base-css.html">Base CSS</a>
                                        </li>
                                        <li class="active">
                                            <a href="./components.html">Components</a>
                                        </li>
                                        <li class="">
                                            <a href="./javascript.html">Javascript plugins</a>
                                        </li>
                                        <li class="">
                                            <a href="./less.html">Using LESS</a>
                                        </li>
                                        <li class="divider-vertical"></li>
                                        <li class="">
                                            <a href="./download.html">Customize</a>
                                        </li>
                                        <li class="">
                                            <a href="./examples.html">Examples</a>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </header><!-- .main-header -->

        <section class="main-section">
            <div class="row">
                <div class="span12">

                    {% block content %}{% endblock %}

                </div>
            </div>

        </section><!-- .main-section -->

        <footer class="main-footer">
            <div class="row">
                <div class="span12">
                    Pied de page
                </div>
            </div>
        </footer><!-- .main-footer -->

    </div><!-- .container -->

    <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"></script>
    <script type="text/javascript" src="{{ asset('bundles/pixelcore/js/twitter/bootstrap-alert.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/pixelcore/js/twitter/bootstrap-button.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/pixelcore/js/twitter/bootstrap-carousel.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/pixelcore/js/twitter/bootstrap-collapse.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/pixelcore/js/twitter/bootstrap-dropdown.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/pixelcore/js/twitter/bootstrap-popover.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/pixelcore/js/twitter/bootstrap-scrollspy.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/pixelcore/js/twitter/bootstrap-tab.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/pixelcore/js/twitter/bootstrap-tooltip.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/pixelcore/js/twitter/bootstrap-transition.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/pixelcore/js/twitter/bootstrap-typeahead.js') }}"></script>
